{
  "name": "create-redux-app",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:8888",
  "scripts": {
    "init": "./init.sh",
    "start": "parallelshell 'yarn run client:watch' 'yarn run server:watch'",
    "client:watch": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint 'src/**/*.{js}'",
    "precommit": "yarn run lint; yarn run prettier;",
    "generate": "plop --plopfile generators/index.js",
    "prettier": "prettier --bracket-spacing --single-quote --tab-width 2 --trailing-comma all --parser babylon --print-width 80  --write \"{{src,webpack},__{tests,mocks}__}/**/*.js\"",
    "server:watch": "NODE_ENV=dev PORT=8888 nodemon --exec babel-node -- ./server/",
    "server:start": "NODE_ENV=production PORT=8888 babel-node ./server/"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "express": "^4.15.2",
    "helmet": "^3.5.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router-dom": "^4.0.0",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.0",
    "redux-thunk": "^2.2.0",
    "styled-components": "^1.4.4"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.24.0",
    "eslint": "^3.17.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-react-app": "^0.6.2",
    "eslint-plugin-flowtype": "^2.30.4",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "flow": "^0.2.3",
    "flow-bin": "^0.42.0",
    "flow-typed": "^2.0.0",
    "husky": "^0.13.2",
    "node-plop": "^0.5.5",
    "nodemon": "^1.11.0",
    "parallelshell": "^2.0.0",
    "plop": "^1.7.4",
    "prettier": "^0.22.0",
    "react-scripts": "0.9.5"
  }
}
